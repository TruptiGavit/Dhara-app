{
  "version": 2,
  "name": "bheri-react-app",
  "rewrites": [
    {
      "source": "/dhara",
      "destination": "https://dhara-flutter-app.vercel.app"
    },
    {
      "source": "/dhara/:path*",
      "destination": "https://dhara-flutter-app.vercel.app/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}

